<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>FTail</id>
    <version>1.0.0</version>
    <title>FTail Log Viewer</title>
    <authors>Baine Wedlock</authors>
    <tags>log tail admin</tags>
    <summary>A simple 'tail'-like app for viewing and following log files.</summary>
    <description>
      ## Usage
      Ftail.exe (filename)

      The App is designed to be called from another app (like FreeCommander) passing the path of a log file.
    </description>
    <projectUrl>https://github.com/bainewedlock/ftail</projectUrl>
    <projectSourceUrl>https://github.com/bainewedlock/ftail</projectSourceUrl>
    <licenseUrl>https://opensource.org/osd-annotated</licenseUrl>
    <iconUrl>https://rawcdn.githack.com/bainewedlock/ftail/3f6429555a8227e74f5a68b4d8af63b18073e9e1/FairyTail/FairyTail.ico</iconUrl>
    <packageSourceUrl>https://github.com/bainewedlock/ftail/tree/master/Chocolatey_Package</packageSourceUrl>
    <dependencies>
      <dependency id="dotnetfx" version="4.6.2"/>
    </dependencies>
  </metadata>
  <files>
    <file src="tools\*.msi" target="tools" />
    <file src="tools\*.ps1" target="tools" />
    <file src="tools\*.txt" target="tools" />
  </files>
</package>
