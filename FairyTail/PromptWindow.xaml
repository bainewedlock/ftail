<Window x:Class="FairyTail.PromptWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FairyTail"
        mc:Ignorable="d"
        Title="PromptWindow" Height="148.017" Width="400"
        WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize"
        >
    <Grid>
        <Label x:Name="TheLabel" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TheTextBox" Height="23" Margin="10,41,10,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top"/>
        <Button Content="Ok" IsDefault="True" HorizontalAlignment="Left" Margin="307,69,0,0" VerticalAlignment="Top" Width="75" Click="Ok_Click"/>
        <Button Content="Cancel" IsCancel="True" HorizontalAlignment="Left" Margin="227,69,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
